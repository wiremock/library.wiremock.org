  },
  "components" : {
    "schemas" : {
      "AnalyzeOperationResult" : {
        "required" : [ "createdDateTime", "lastUpdatedDateTime", "status" ],
        "type" : "object",
        "properties" : {
          "analyzeResult" : {
            "$ref" : "#/components/schemas/AnalyzeResult"
          },
          "createdDateTime" : {
            "type" : "string",
            "description" : "Date and time (UTC) when the analyze operation was submitted.",
            "format" : "date-time",
            "nullable" : false
          },
          "lastUpdatedDateTime" : {
            "type" : "string",
            "description" : "Date and time (UTC) when the status was last updated.",
            "format" : "date-time",
            "nullable" : false
          },
          "status" : {
            "$ref" : "#/components/schemas/OperationStatus"
          }
        },
        "description" : "Status and result of the queued analyze operation."
      },
      "AnalyzeResult" : {
        "required" : [ "readResults", "version" ],
        "type" : "object",
        "properties" : {
          "documentResults" : {
            "type" : "array",
            "description" : "Document-level information extracted from the input.",
            "items" : {
              "$ref" : "#/components/schemas/DocumentResult"
            }
          },
          "errors" : {
            "type" : "array",
            "description" : "List of errors reported during the analyze operation.",
            "items" : {
              "$ref" : "#/components/schemas/ErrorInformation"
            }
          },
          "pageResults" : {
            "type" : "array",
            "description" : "Page-level information extracted from the input.",
            "items" : {
              "$ref" : "#/components/schemas/PageResult"
            }
          },
          "readResults" : {
            "type" : "array",
            "description" : "Text extracted from the input.",
            "items" : {
              "$ref" : "#/components/schemas/ReadResult"
            }
          },
          "version" : {
            "type" : "string",
            "description" : "Version of schema used for this result."
          }
        },
        "description" : "Analyze operation result."
      },
      "BoundingBox" : {
        "maxItems" : 8,
        "minItems" : 8,
        "type" : "array",
        "description" : "Quadrangle bounding box, with coordinates specified relative to the top-left of the original image. The eight numbers represent the four points, clockwise from the top-left corner relative to the text orientation. For image, the (x, y) coordinates are measured in pixels. For PDF, the (x, y) coordinates are measured in inches.",
        "items" : {
          "type" : "number",
          "nullable" : false
        }
      },
      "Confidence" : {
        "maximum" : 1,
        "minimum" : 0,
        "type" : "number",
        "description" : "Confidence value.",
        "nullable" : false
      },
      "DataTable" : {
        "required" : [ "cells", "columns", "rows" ],
        "type" : "object",
        "properties" : {
          "cells" : {
            "type" : "array",
            "description" : "List of cells contained in the table.",
            "items" : {
              "$ref" : "#/components/schemas/DataTableCell"
            }
          },
          "columns" : {
            "minimum" : 1,
            "type" : "integer",
            "description" : "Number of columns.",
            "nullable" : false
          },
          "rows" : {
            "minimum" : 1,
            "type" : "integer",
            "description" : "Number of rows.",
            "nullable" : false
          }
        },
        "description" : "Information about the extracted table contained in a page."
      },
      "DataTableCell" : {
        "required" : [ "boundingBox", "columnIndex", "confidence", "rowIndex", "text" ],
        "type" : "object",
        "properties" : {
          "boundingBox" : {
            "$ref" : "#/components/schemas/BoundingBox"
          },
          "columnIndex" : {
            "minimum" : 0,
            "type" : "integer",
            "description" : "Column index of the cell.",
            "nullable" : false
          },
          "columnSpan" : {
            "minimum" : 1,
            "type" : "integer",
            "description" : "Number of columns spanned by this cell.",
            "nullable" : false
          },
          "confidence" : {
            "$ref" : "#/components/schemas/Confidence"
          },
          "elements" : {
            "type" : "array",
            "description" : "When includeTextDetails is set to true, a list of references to the text elements constituting this table cell.",
            "items" : {
              "$ref" : "#/components/schemas/ElementReference"
            }
          },
          "isFooter" : {
            "type" : "boolean",
            "description" : "Is the current cell a footer cell?",
            "nullable" : false,
            "default" : false
          },
          "isHeader" : {
            "type" : "boolean",
            "description" : "Is the current cell a header cell?",
            "nullable" : false,
            "default" : false
          },
          "rowIndex" : {
            "minimum" : 0,
            "type" : "integer",
            "description" : "Row index of the cell.",
            "nullable" : false
          },
          "rowSpan" : {
            "minimum" : 1,
            "type" : "integer",
            "description" : "Number of rows spanned by this cell.",
            "nullable" : false
          },
          "text" : {
            "type" : "string",
            "description" : "Text content of the cell."
          }
        },
        "description" : "Information about the extracted cell in a table."
      },
      "DocumentResult" : {
        "required" : [ "docType", "fields", "pageRange" ],
        "type" : "object",
        "properties" : {
          "docType" : {
            "type" : "string",
            "description" : "Document type."
          },
          "fields" : {
            "type" : "object",
            "additionalProperties" : {
              "$ref" : "#/components/schemas/FieldValue"
            },
            "description" : "Dictionary of named field values."
          },
          "pageRange" : {
            "maxItems" : 2,
            "minItems" : 2,
            "type" : "array",
            "description" : "First and last page number where the document is found.",
            "items" : {
              "minimum" : 1,
              "type" : "integer",
              "nullable" : false
            }
          }
        },
        "description" : "A set of extracted fields corresponding to the input document."
      },
      "ElementReference" : {
        "type" : "string",
        "description" : "Reference to a line or word."
      },
      "ErrorInformation" : {
        "required" : [ "code", "message" ],
        "type" : "object",
        "properties" : {
          "code" : {
            "type" : "string"
          },
          "message" : {
            "type" : "string"
          }
        }
      },
      "ErrorResponse" : {
        "required" : [ "error" ],
        "type" : "object",
        "properties" : {
          "error" : {
            "$ref" : "#/components/schemas/ErrorInformation"
          }
        }
      },
      "FieldValue" : {
        "required" : [ "type" ],
        "type" : "object",
        "properties" : {
          "boundingBox" : {
            "$ref" : "#/components/schemas/BoundingBox"
          },
          "confidence" : {
            "$ref" : "#/components/schemas/Confidence"
          },
          "elements" : {
            "type" : "array",
            "description" : "When includeTextDetails is set to true, a list of references to the text elements constituting this field.",
            "items" : {
              "$ref" : "#/components/schemas/ElementReference"
            }
          },
          "page" : {
            "minimum" : 1,
            "type" : "integer",
            "description" : "The 1-based page number in the input document.",
            "nullable" : false
          },
          "text" : {
            "type" : "string",
            "description" : "Text content of the extracted field."
          },
          "type" : {
            "$ref" : "#/components/schemas/FieldValueType"
          },
          "valueArray" : {
            "type" : "array",
            "description" : "Array of field values.",
            "items" : {
              "$ref" : "#/components/schemas/FieldValue"
            }
          },
          "valueDate" : {
            "type" : "string",
            "description" : "Date value.",
            "format" : "date-time",
            "nullable" : false
          },
          "valueInteger" : {
            "type" : "integer",
            "description" : "Integer value.",
            "nullable" : false
          },
          "valueNumber" : {
            "type" : "number",
            "description" : "Floating point value.",
            "nullable" : false
          },
          "valueObject" : {
            "type" : "object",
            "additionalProperties" : {
              "$ref" : "#/components/schemas/FieldValue"
            },
            "description" : "Dictionary of named field values."
          },
          "valuePhoneNumber" : {
            "type" : "string",
            "description" : "Phone number value."
          },
          "valueString" : {
            "type" : "string",
            "description" : "String value."
          },
          "valueTime" : {
            "type" : "string",
            "description" : "Time value.",
            "format" : "date-time",
            "nullable" : false
          }
        },
        "description" : "Recognized field value."
      },
      "FieldValueType" : {
        "type" : "string",
        "description" : "Semantic data type of the field value.",
        "nullable" : false,
        "enum" : [ "string", "date", "time", "phoneNumber", "number", "integer", "array", "object" ]
      },
      "FormFieldsReport" : {
        "required" : [ "accuracy", "fieldName" ],
        "type" : "object",
        "properties" : {
          "accuracy" : {
            "type" : "number",
            "description" : "Estimated extraction accuracy for this field.",
            "nullable" : false
          },
          "fieldName" : {
            "type" : "string",
            "description" : "Training field name."
          }
        },
        "description" : "Report for a custom model training field."
      },
      "KeyValueElement" : {
        "required" : [ "text" ],
        "type" : "object",
